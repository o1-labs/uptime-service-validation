# PostgreSQL Database Configuration
POSTGRES_HOST=host.containers.internal
POSTGRES_PORT=5432
POSTGRES_DB=coordinator
POSTGRES_USER=o1labs
POSTGRES_PASSWORD='PASSWORD_HERE'
POSTGRES_SSLMODE=disable

# AWS Configuration
AWS_REGION=us-west-2
AWS_S3_BUCKET=o1labs-uptime-service-backend
AWS_ACCESS_KEY_ID=aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=aws_secret_access_key_here

# Network Configuration
PLATFORM=
NETWORK_NAME=mainnet

# Container Runtime Configuration (for local testing with TEST_ENV=1)
CONTAINER_RUNTIME=docker

# Google Sheets Configuration (optional for local testing)
SPREADSHEET_NAME='Mina Foundation Delegation Application (Responses)'
SPREADSHEET_CREDENTIALS_JSON=/usr/src/app/gcloud/gcloud.json.test
IGNORE_APPLICATION_STATUS=1

# Google Cloud Credentials Path (local file system)
GOOGLE_CREDENTIALS_PATH=./gcloud.json.test